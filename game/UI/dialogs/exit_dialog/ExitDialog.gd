extends WindowDialog

# -------------------------------------------------------------------------------------------------
signal cancel
signal exit

# -------------------------------------------------------------------------------------------------
var project_ids: Array

# -------------------------------------------------------------------------------------------------
func set_text(text: String) -> void:
	$Label.text = text

# -------------------------------------------------------------------------------------------------
func _on_CancelButton_pressed(): 
	hide()
	emit_signal("cancel")

# -------------------------------------------------------------------------------------------------	
func _on_ExitButton_pressed(): 
	emit_signal("exit")
