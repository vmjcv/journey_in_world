; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "ArrayUtils",
"language": "GDScript",
"path": "res://addons/SIsilicon.3d.text/array_utils.gd"
}, {
"base": "MultiMeshInstance",
"class": "HexCell",
"language": "GDScript",
"path": "res://script/common/hex_cell.gd"
}, {
"base": "Spatial",
"class": "HexGrid",
"language": "GDScript",
"path": "res://script/common/hex_grid.gd"
}, {
"base": "Resource",
"class": "HexStatic",
"language": "GDScript",
"path": "res://script/common/hex_static.gd"
} ]
_global_script_class_icons={
"ArrayUtils": "",
"HexCell": "",
"HexGrid": "",
"HexStatic": ""
}

[application]

config/name="journey_in_world"
run/main_scene="res://scene/main.tscn"
config/icon="res://icon.png"

[display]

window/size/width=1920
window/size/height=1080
window/size/always_on_top=true
window/size/test_width=960
window/size/test_height=540

[editor_plugins]

enabled=PoolStringArray( "SIsilicon.3d.text" )

[rendering]

environment/default_environment="res://default_env.tres"
